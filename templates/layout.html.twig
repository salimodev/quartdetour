              function opensidebar(){
                  var elem = document.getElementById('menumobilenav')
               $('#menumobilenav').each(function(){
                   $(this).removeClass('active');
               }); 
              $('#menumobilenav').addClass('active');
             }
           
            function closenavbar(){
